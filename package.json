    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.7.4",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "diff": "^7.0.0",
    "fs-extra": "^11.2.0",
    "ora": "^8.1.0",
    "pathe": "^1.1.2",
    "simple-git": "^3.27.0"
